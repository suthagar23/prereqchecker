<%- include ('partials/header') -%>

<div class="container logo-container py-5 d-flex justify-content-center">
    <h1 class="display-6 mb-2" style="color:#ffffff!important;">Enrollments</h1>
</div>

<div class="container container-header">
<!-- <header class="jumbotron"> -->
    <div class = "container py-5">
        <!-- <h1 class="display-4">True Legacy Homes Sale Manager</h1> -->
        <!-- <div class="text-center">
            <h1 class="display-6 mb-2">Enrollments</h1>
        </div> -->
            
            <!-- View Sales -->
            <table class="table table-striped table-bordered table-expandable mydatatable" style="width: 100%;">
                <thead>
                    <tr>
                      <th scope="col">Event ID</th>
		              <th scope="col">Enrollment ID</th>
		              <th scope="col">Email</th>
		              <th scope="col">Name</th>
		              <th scope="col">Status</th>
		              <th scope="col">Course Name</th>
		              <th scope="col">Contact ID</th>
                    </tr>
                </thead>
                
                <tbody>
                    <% enrollmentresults.forEach(function(enrollmentresult) { %> 
                        <tr>
                            <td><%= enrollmentresult.event_id %></td>
                            <td><%= enrollmentresult.enrollment_id %></td>
                            <td><%= enrollmentresult.email %></td>
                            <td><%= enrollmentresult.firstName + " " + enrollmentresult.lastName %></td>
                            <td><%= enrollmentresult.status %></td>
                            <td><%= enrollmentresult.name %></td>
                            <td><%= enrollmentresult.contact_id %></td>
                        </tr>
                    <% }); %>
                </tbody>
                
            </table>    

   </div>        
</div>




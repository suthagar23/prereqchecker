            <!-- View Sales -->
            <table class="table table-striped table-bordered table-expandable mydatatable" style="width: 100%;">
                <thead>
                    <tr>
                      <th scope="col">Event ID</th>
		              <th scope="col">Enrollment ID</th>
		              <th scope="col">Email</th>
		              <th scope="col">Name</th>
		              <th scope="col">Status</th>
		              <th scope="col">Score</th>
		              <th scope="col">URL Name</th>
                    </tr>
                </thead>
                
                <tbody>
                    <% learndotresults.forEach(function(learndotresult) { %> 
                        <tr>
                            <td><%= learndotresult.event_id %></td>
                            <td><%= learndotresult.enrollment_id %></td>
                            <td><%= learndotresult.email %></td>
                            <td><%= learndotresult.firstName + " " + learndotresult.lastName %></td>
                            <td><%= learndotresult.status %></td>
                            <td><%= learndotresult.score %></td>
                            <td><%= learndotresult.urlname %></td>
                        </tr>
                    <% }); %>
                </tbody>
                
            </table>    

   </div>        
